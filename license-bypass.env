# n8n License Bypass Configuration
# Source this file before starting n8n: source license-bypass.env && npm start

# Option 1: Use mock license server
export N8N_LICENSE_SERVER_URL=http://localhost:3001

# Option 2: Set static license certificate (bypasses server communication)
export N8N_LICENSE_CERT=''

# Option 3: Disable license auto-renewal
export N8N_LICENSE_AUTO_RENEW_ENABLED=false

# Option 4: Set tenant ID (different tenant IDs may have different restrictions)
export N8N_LICENSE_TENANT_ID=999

# Option 5: Disable license server communication entirely
# export N8N_LICENSE_SERVER_URL=""

# Additional development settings
export N8N_DEV_RELOAD=true
export NODE_ENV=development

echo "üîì License bypass configuration loaded"
echo "üìù Available options:"
echo "   1. Mock license server (requires running mock-license-server.js)"
echo "   2. Static license certificate (bypasses server entirely)"
echo "   3. Disabled auto-renewal"
echo "   4. Custom tenant ID"
echo ""
echo "üöÄ Start n8n with: npm start"
